{
    "name": "jclaveau/php-types",
    "description": "",
    "license": "MIT",
    "type": "library",
    "keywords": ["type", "number", "string"],
    "authors": [
        {
            "name": "Jean Claveau",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4" : {
            "JClaveau\\" : "src"
        },
        "files": [
              "src/polyfills.php",
              "src/functions.php"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jclaveau/php-visibility-violator",
            "no-api": true
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "5.*",
        "jclaveau/phpunit-profile-asserts": "^1.0"
    },
    "require": {
        "php": ">=5.6.0",
        "jclaveau/php-deferred-callchain": "^1.0",
        "jclaveau/php-immutable-trait": "^1.0"
    }
}
